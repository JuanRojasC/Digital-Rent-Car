							"    pm.collectionVariables.set(\"last_name_user\",res.lastName);\r",
							"    pm.collectionVariables.set(\"email_user\",res.email);\r",
							"    pm.collectionVariables.set(\"password_user\",res.password);\r",
							"    pm.expect(res.user.name).to.eql(\"Sergio\");\r",
							"    pm.expect(res.lastName).to.eql(\"Garcia\");\r",
							"    pm.expect(res.user.email).to.eql(\"sergiogarcia@email.com\");\r",
							"    pm.expect(res.password).to.eql(\"sergiogarcia\");\r",
